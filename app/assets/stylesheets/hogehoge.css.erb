.example {
  width: 100%;
  height: 400px;
  color: #fff;
  background-image: url("./BO151030535008_TP_V.jpg");
  background-size: cover;
  background-position: center;
  font-family: serif;
  margin-top: 10px;
  padding-bottom: 100px;
}
.contents {
  background-color: rgba(0,0,0,0.6);
  width: 100%;
  height: 400px;
}


@media (min-width: 768px) {

  .featurette-heading {
    font-size: 50px;
  }
}

@media (min-width: 992px) {
  .featurette-heading .featurette-image {
    margin-top: 50px;
  }
}

.featurette-divider {
  margin: 80px 0; /* Space out the Bootstrap <hr> more */
}

/* Thin out the marketing headings */
.featurette-heading {
  font-weight: 300;
  line-height: 1;
}
.featurette-image {
  background-color: rgba(0,0,0,0.6);
}
